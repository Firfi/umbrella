{
	"name": "@thi.ng/oquery",
	"version": "2.1.42",
	"description": "Datalog-inspired, optimized pattern/predicate query engine for JS objects & arrays",
	"type": "module",
	"module": "./index.js",
	"typings": "./index.d.ts",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/thi-ng/umbrella.git"
	},
	"homepage": "https://github.com/thi-ng/umbrella/tree/develop/packages/oquery#readme",
	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/postspectacular"
		},
		{
			"type": "patreon",
			"url": "https://patreon.com/thing_umbrella"
		}
	],
	"author": "Karsten Schmidt (https://thi.ng)",
	"license": "Apache-2.0",
	"scripts": {
		"build": "yarn clean && tsc --declaration",
		"clean": "rimraf --glob '*.js' '*.d.ts' '*.map' doc",
		"doc": "typedoc --excludePrivate --excludeInternal --out doc src/index.ts",
		"doc:ae": "mkdir -p .ae/doc .ae/temp && api-extractor run --local --verbose",
		"doc:readme": "yarn doc:stats && tools:readme",
		"doc:stats": "tools:module-stats",
		"pub": "yarn npm publish --access public",
		"test": "testament test"
	},
	"dependencies": {
		"@thi.ng/api": "^8.9.1",
		"@thi.ng/checks": "^3.4.1",
		"@thi.ng/defmulti": "^2.1.40",
		"@thi.ng/equiv": "^2.1.26"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.36.3",
		"@thi.ng/testament": "^0.3.19",
		"rimraf": "^5.0.1",
		"tools": "workspace:^",
		"typedoc": "^0.24.8",
		"typescript": "^5.1.6"
	},
	"keywords": [
		"array",
		"datalog",
		"graph",
		"intersection",
		"object",
		"pattern",
		"predicate",
		"query",
		"triples",
		"typescript",
		"union",
		"wildcard"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=12.7"
	},
	"files": [
		"./*.js",
		"./*.d.ts"
	],
	"exports": {
		".": {
			"default": "./index.js"
		},
		"./api": {
			"default": "./api.js"
		},
		"./query": {
			"default": "./query.js"
		}
	},
	"thi.ng": {
		"related": [
			"associative",
			"csv",
			"egf",
			"rstream-query"
		],
		"year": 2020
	}
}
